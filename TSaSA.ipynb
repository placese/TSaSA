    "import time\n",
    "import pandas as pd\n",
    "from mlxtend.preprocessing import TransactionEncoder\n",
    "from mlxtend.frequent_patterns import apriori, fpmax, fpgrowth\n",
    "\n",
    "\n",
    "dataset = [['Бинт', 'Перекись', 'Зеленка', 'Мазь'],\n",
    "                ['Пенталгин', 'Ношпа', 'Ибупрофен'],\n",
    "                ['Перекись', 'Хлоргексидин', 'Йод'],\n",
    "                ['Ношпа', 'Пенталгин', 'Микстура', 'Мазь'],\n",
    "                ['Зубная паста', 'Зубная щетка', 'Мыло'],\n",
    "                ['Гематоген'],\n",
    "                ['Активированный уголь', 'Линекс'],\n",
    "                ['Клизма', 'Мыло'],\n",
    "                ['Шприцы', 'Расствор', 'Спиртовые салфетки', 'Ношпа'],\n",
    "                ['Хлоргексидин', 'Зеленка', 'Бинт', 'Мазь'],\n",
    "                ['Ибупрофен', 'Арбидол', 'Маска'],\n",
    "                ['Маска', 'Антисептик'],\n",
    "                ['Маска', 'Мыло', 'Зубная паста', 'Лечебный шампунь'],\n",
    "                ['Арбидол', 'Линекс'],\n",
    "                ['Антисептик', 'Маска', 'Гематоген', 'Пенталгин'],\n",
    "                ['Маска', 'Мыло', 'Перекись', 'Мазь'],\n",
    "                ['Арбидол', 'Ибупрофен', 'Градусник'],\n",
    "                ['Зодак', 'Ингалатор', 'Элаком', 'Маск', 'Антисептик'],\n",
    "                ['Хлоргексидин', 'Бинт', 'Шприцы', 'Мыло', 'Спиртовые салфетки'],\n",
    "                ['Маска', 'Мыло', 'Антисептик']]\n",
    "\n",
    "# @time_dec\n",
    "# def calculate(transactions):\n",
    "#     return fpgrowth(itemSetList, minSupRatio=0.1, minConf=0.8)\n",
    "\n",
    "# result, time_2 = calculate(dataset)\n",
    "# freqItemSet, rules = result\n",
    "# print(freqItemSet)\n",
    "# print()\n",
    "# print(rules)\n",
    "# print()\n",
    "# print(f'Время обработки {time_2} секунду')\n",
    "\n",
    "start_time = time.time()\n",
    "te = TransactionEncoder()\n",
    "te_ary = te.fit(dataset).transform(dataset)\n",
    "df = pd.DataFrame(te_ary, columns=te.columns_)\n",
    "print(f'Время обработки {time.time() - start_time} секунду')\n
    "\n",
    "frequent_itemsets = fpgrowth(df, min_support=0.1, use_colnames=True)\n",
    "frequent_itemsets"
   ]
