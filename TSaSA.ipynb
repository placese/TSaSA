    "import time\n",
    "\n",
    "def time_dec(func):\n",
    "    def wrapper(*args, **kwargs):\n",
    "        start = time.time()\n",
    "        result = func(*args, **kwargs)\n",
    "        end = time.time() - start\n",
    "        return result, end\n",
    "    return wrapper"
   ]
